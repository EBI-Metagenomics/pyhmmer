#!/usr/bin/env fish

hyperfine (for x in (seq 1 12); echo "python -m pyhmmer.hmmer --jobs $x hmmsearch Pfam.v33-1.hmm tests/data/seqs/938293.PRJEB85.HG003687.faa"; end) (for x in (seq 1 12); echo "hmmsearch --cpu $x Pfam.v33-1.hmm tests/data/seqs/938293.PRJEB85.HG003687.faa"; end) (for x in (seq 1 12); echo "python -m pyhmmer.hmmer --jobs $x hmmsearch Pfam.v33-1.pressed.hmm tests/data/seqs/938293.PRJEB85.HG003687.faa"; end) (for x in (seq 1 12); echo "hmmsearch --cpu $x Pfam.v33-1.pressed.hmm tests/data/seqs/938293.PRJEB85.HG003687.faa"; end) (for x in (seq 1 12); echo "hmmscan --cpu $x Pfam.v33-1.pressed.hmm tests/data/seqs/938293.PRJEB85.HG003687.faa"; end) --export-json benches/hmmsearch.json -r 4 -w 1
hyperfine (for x in (seq 1 12); echo "python -m pyhmmer.hmmer --jobs $x phmmer tests/data/seqs/938293.PRJEB85.HG003687.faa tests/data/seqs/938293.PRJEB85.HG003687.faa"; end) (for x in (seq 1 12); echo "phmmer --cpu $x tests/data/seqs/938293.PRJEB85.HG003687.faa tests/data/seqs/938293.PRJEB85.HG003687.faa"; end) --export-json benches/phmmer.json -r 4 -w 1